pipeline {
   agent any

   stages {
      stage('Copy artifact') {
         steps {
            copyArtifacts filter: 'example1', fingerprintArtifacts: true, projectName: 'example 1', selector: lastSuccessful()
         }
      }
      stage('deliver') {
         steps {
             sshagent(['toolbox-vagrant-key']) {
                sh "scp example1 vagrant@10.10.50.3:~"
             }
         }
      }  
   }
}

